<div class="list-wrap">
    <p class="title">
        <%=data.title%>
    </p>
    <ul class="list">
        <%data.list.forEach(function(item){%>
            <li>
                <a href="<%=item.href%>">
                    <img src="<%=item.previewImage%>" alt="">
                </a>
            </li>
            <%})%>
    </ul>
</div>