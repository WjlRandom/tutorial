<div class="register-form">
    <div class="form-main">
        <div class="form-item">
            <span class="item-left">姓名</span>
            <input type="text" class="user-name" placeholder="请输入姓名" />
        </div>
        <div class="form-item">
            <span class="item-left">手机号</span>
            <input type="phone" class="phone" placeholder="手机号码要求唯一" maxlength="11" />
        </div>
        <div class="form-item code-item">
            <span class="code-input">短信验证码</span> <input type="phone" class="code" maxlength="4" />
            <button class="code-btn">获取验证码</button>
        </div>
    </div>
    <div class="form-submit">
        <div class="common-btn submit-btn ">
            <%=data%>
        </div>
    </div>
</div>